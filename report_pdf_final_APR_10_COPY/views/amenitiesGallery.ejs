<style>
    .amenities_root_gallery {
        margin-left: 65px;
        margin-right: 65px;
        background-color: #FCFDFC;
    }

    .amenities_card {
        display: grid;
        margin-top: 44px;
    }

    .amenities_comment_item {
        display: flex;
        align-items: center;
        justify-content: space-between;
        margin-bottom: 8px;
        margin-top: 8px;
    }

    .amenities_comment_div {
        margin-top: 31px;
        margin-bottom: 48px;
    }

    .amenities_comment {
        color: #000;
        font-size: 14px;
        font-style: normal;
        font-weight: 600;
        line-height: normal;
        font-family: "Noto Sans";

    }

    .amenities_comment_text {
        color: #000;
        font-size: 14px;
        font-style: normal;
        font-weight: 400;
        line-height: normal;
        font-family: "Noto Sans";

    }

    .amenities_comment_date_text {
        color: rgba(0, 0, 0, 0.50);
        text-align: right;
        font-size: 14px;
        font-weight: 400;
        line-height: normal;
        font-family: "Noto Sans";

    }

    .amenities_hr {
        border-top: 1px dotted #f2f3f2;
    }

    .amenities_comment_section {
        margin-top: 8px;
    }

    .amenities_grid-container1 {
        display: grid;
        grid-template-columns: repeat(4, 1fr);
        grid-gap: 10px;
        /* margin-top: 40px; */
    }

    .amenities_asset_img {
        border-radius: 16px;
        height: 170px;
        width: 100%;
        object-fit: cover
    }
</style>
<div class="amenities_root_gallery">

<div class="amenities_grid-container1">
    <% for (let i=0; i < amenitiesTableData.cards.length; i++) { %>
        <div class="grid-item" class="amenities_card">
            <img src="<%= amenitiesTableData?.cards[i]?.imageSrc %>" class="amenities_asset_img" />
        </div>
        <% } %>
</div>
</div>