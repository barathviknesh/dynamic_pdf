<style>
     .rootGoogle {
        margin-left: 65px;
        margin-right: 65px;
        background-color: #FCFDFC;

    }
    .assetImg {
        border-radius: 16px;
        height: 240px;
        width: 100%;
        object-fit: cover
    }

    .date {
        margin-top: 16px;
        color: rgba(0, 0, 0, 0.50);
        font-size: 16px;
        font-style: normal;
        font-weight: 600;
        line-height: normal;
        font-family: "Noto Sans";

    }

    .googleEarth {
        align-items: center;
        display: flex;
        justify-content: center;
        margin-top: 40px;
        margin-bottom: 40px;
    }

    .googletitle {
        color: #000;
        font-size: 24px;
        font-style: normal;
        font-weight: 700;
        line-height: normal;
        font-family: "Noto Sans";

    }

    .grid-item {
        display: grid;
    }
</style>
<div class="rootGoogle">

    <div class="googleEarth">
        <h class="googletitle">Google Earth Comparison</h>
    </div>
    <div class="grid-container">
        <% for (let i=0; i < googleEarth?.length; i++) { %>
            <div class="grid-item" class="cardBox">
                <img src="<%= googleEarth[i]?.imageSrc %>" class="assetImg" />
                <h class="date">
                    <%= googleEarth[i]?.date %>
                </h>

            </div>
            <% } %>
    </div>

</div>

<script>

</script>